{
    "AWSEBSDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "mandartelitech/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "mandartelitech/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128

        },
        {
            "name": "server",
            "image": "mandartelitech/multi-server",
            "hostname": "api",
            "essential": false,
            "memory": 128

        },
        {
            "name": "ngnix",
            "image": "mandartelitech/multi-ngnix",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client","server"]

        }
    ]
}